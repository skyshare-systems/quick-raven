{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "module": "esnext"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "./components/**/*.ts",
    "./components/**/*.tsx"
  ],
  "exclude": ["node_modules"]
}
