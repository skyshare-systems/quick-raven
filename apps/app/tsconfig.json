{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "noImplicitAny": false,
    "baseUrl": ".",
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],

    "module": "esnext",
    "target": "ESNext",
    "lib": ["ES2017", "DOM", "DOM.Iterable", "ESNext"]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "./components/**/*.ts",
    "./components/**/*.tsx"
  ],
  "exclude": ["node_modules"]
}
